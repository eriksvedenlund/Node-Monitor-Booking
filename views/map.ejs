        var ostrahamngatanInfo = '<h5>Östra hamngatan</h5>' + '<h6>Price: 10000 kr</h6>' + '<img src="./images/ostrahamngatan.jpg" style="width:225px"/>' +'<br><a href="/monitors">Book</a>';
        var gronsakstorgetInfo = '<h5>Grönsakstorget</h5>' + '<h6>Price: 6000 kr</h6>' + '<img src="./images/gronsakstorget.jpg" style="width:225px"/>' +'<br><a href="/monitors">Book</a>';
