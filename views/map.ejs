          content: kungsportsplatsenInfo
        });
        var easterIslandWindow = new google.maps.InfoWindow({
          content: easterIslandInfo
        });

        function closeAll(){
          centralenWindow.close(map, this);
          korsvagenWindow.close(map, this);
          svingelnWindow.close(map, this);
          ostrahamngatanWindow.close(map, this);
          gronsakstorgetWindow.close(map, this);
          vasastanWindow.close(map, this);
          gotaplatsenWindow.close(map, this);
          kungsportsplatsenWindow.close(map, this);
          brunnsparkenWindow.close(map, this);
        }



        google.maps.event.addListener(brunnsparkenMarker, 'click', function() {
          // Calling the open method of the infoWindow
          closeAll();
          brunnsparkenWindow.open(map, this);
        });
        google.maps.event.addListener(centralenMarker, 'click', function() {
          // Calling the open method of the infoWindow
          closeAll();
          centralenWindow.open(map, this);
        });
        google.maps.event.addListener(korsvagenMarker, 'click', function() {
          // Calling the open method of the infoWindow
          closeAll();
          korsvagenWindow.open(map, this);
        });
        google.maps.event.addListener(svingelnMarker, 'click', function() {
          // Calling the open method of the infoWindow
          closeAll();
          svingelnWindow.open(map, this);
        });
